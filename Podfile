use_frameworks!

target 'SwiftLearning' do
  
  # Map
  pod 'BaiduMapKit', '~> 5.4.0'
  # layout
  pod 'SnapKit', '~> 4.0.1'
  # network
  pod 'Alamofire', '~> 4.4.0'
  pod 'Moya', '~> 13.0.1'
  # image
  pod 'Kingfisher', '~> 5.6.0'
  # json -> dict
  pod 'SwiftyJSON', '~> 5.0.0'
  # json -> model
  pod 'ObjectMapper', '~> 3.5.3'
  pod 'HandyJSON', '~> 5.0.2'
  
  pod 'RxSwift', '~> 4.5.0'
  # refresh
  pod 'PullToRefresher', '~> 3.3'
  pod 'DGElasticPullToRefresh', '~> 1.1'
  # storage
  pod 'SQLite.swift', '~> 0.12.2'
  pod 'RealmSwift', '~> 4.1.1'
  
end
